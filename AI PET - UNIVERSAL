--Hello user, this script is universal but might get detected in some anti cheats. Use at own risk, Version 1.0. I will post when I update this on my youtube channel: https://www.youtube.com/channel/UC87a_NximlQBvFLGs2BzcZw --

-- MADE BY @KING_DOGEDEV

-- Commands: --
--up, makes the robot slowly go up--
--stop, stops the robot in the air after using up.--
--down, makes the robot stop floating and go to the floor.--
--no legs, makes the robots legs have no collision--
--legs, makes the robots legs fixed after no legs command--
--sit, makes the robot sit.--
--get up, makes the robot no longer sit, does not make the robot jump.--
--jump, makes the robot jump--
--copy chat, copies the host's and overseer's chat until you say stop copy chat--
--stop copy chat, stops the robot from copying chat--
--copy next chat, copies the next chat and the next chat only.--
--/e copy next chat, copies the next chat in secret --

-- Information:--
-- The overseer is a person who watches the bot but dosent have full control, TIP: if you let someone else control the bot then put them as the host and you as the over seer, if you want to be the host then put the over seer and nil without "", The host has commands and has full control of the robot, If the host or the overseer dosent exist then the robot will probably break --



 -- UNIVERSAL --
local Humanoid = game.Players.LocalPlayer.Character.Humanoid
local float = false
local host = "9BitByte"
local overseer = "King_DogeDev"
local replicatedStorage = game:GetService("ReplicatedStorage")
local textChatService = game:GetService("TextChatService")
local copychat = false
local copynextchat = false
local enabled = true
local ver = 1.0


















































local function Chat(msg)
	if replicatedStorage:FindFirstChild('DefaultChatSystemChatEvents') then
		replicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
	else
		textChatService.TextChannels.RBXGeneral:SendAsync(msg)


	end
end


game:GetService("Players")[host].chatted:Connect(function(msg)

	if msg:lower() == "information" then
		Chat("Dohickey Control || Made by King_DogeDev/Thing")
wait(1)
		Chat("Main controler:  ".. host .."  Overseer:  ".. overseer)
wait(1)
        Chat("Version:  ".. ver)
	end
end)

game:GetService("Players")[host].chatted:Connect(function(msg)

	if msg:lower() == "stop copy chat" then
		copychat = false
	end
end)

game:GetService("Players")[host].chatted:Connect(function(msg)
	if msg:lower() == "copy chat" then
		copychat = true
	end
end)

game:GetService("Players")[host].chatted:Connect(function(msg)
	if msg:lower() == "copy next chat" then
		copynextchat = true
	end
end)

game:GetService("Players")[host].chatted:Connect(function(msg)
	if msg:lower() == "/e copy next chat" then
		copynextchat = true
	end
end)

game:GetService("Players")[host].chatted:Connect(function(msg)
	if copynextchat == true then
		copynextchat = false
		Chat(msg)

	end
end)

game:GetService("Players")[host].chatted:Connect(function(msg)
	if copychat == true then



			Chat(msg)

	end
end)


game:GetService("Players")[host].chatted:Connect(function(msg)
	if msg:lower() == "sit" then
		Humanoid.Sit = true
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "get up" then
		Humanoid.Sit = false
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "jump" then
		Humanoid.Jump = true
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "come" then
		Chat("Of course!")
		local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
		rootPart.CFrame = game.Players[host].Character.HumanoidRootPart.CFrame
wait(0.02)
		rootPart.CFrame = game.Players[host].Character.HumanoidRootPart.CFrame
wait(0.02)
		rootPart.CFrame = game.Players[host].Character.HumanoidRootPart.CFrame
wait(0.02)
		rootPart.CFrame = game.Players[host].Character.HumanoidRootPart.CFrame

	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "down" then
float = false
		Humanoid.HipHeight = 0
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "up" then
		Chat("Say stop to stop floating up and down to go to the ground")
float = true
while true do task.wait(0.01)
if float == true then 

		Humanoid.HipHeight = Humanoid.HipHeight + 0.1

			end
       end
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "stop" then
		float = false
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "no legs" then
		Humanoid.PlatformStand = true
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "legs" then
		Humanoid.PlatformStand = false
	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "reset" then
		Chat("Made by King_DogeDev, Hope you had fun")
		Humanoid.Health = 0
		local er = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		wait(5)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		wait(0.05)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		wait(0.05)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		script:Destroy()

	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "terminate" then
		Chat("Made by King_DogeDev, Hope you had fun")
		Humanoid.Health = 0
		local er = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		wait(5)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		wait(0.05)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		wait(0.05)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		script:Destroy()

	end
end)

game:GetService("Players")[host].Chatted:Connect(function(msg)
	if msg:lower() == "kill" then
		Chat("Made by King_DogeDev, Hope you had fun")
		Humanoid.Health = 0
local er = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
wait(5)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		wait(0.05)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
		wait(0.05)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = er
script:Destroy()

	end
end)




